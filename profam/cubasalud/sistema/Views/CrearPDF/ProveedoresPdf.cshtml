@{
    Layout = "_HojaLayout";

    DateTime fecha = DateTime.Now;


}

@model IEnumerable<Database.Shared.Models.Proveedor>

<img width="40" src="/assets/images/farmamestlogo.png" alt="">
<div class="text-right" style="font-size: 12px;">
    <label class=""><strong>Fecha y Hora de Emision: </strong>@fecha</label> &nbsp; &nbsp;
    <br />
</div>

    <table class=" table">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Giro</th>
                <th>Telefono</th>
                <th>Nit</th>
                <th>Direccion</th>
                <th>Correo</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nombre</td>
                    <td>@item.Giro</td>
                    <td>@item.Telefono_1</td>
                    <td>@item.Nit</td>
                    <td>@item.Direccion</td>
                    <td>@item.Correo</td>

                </tr>
            }
        </tbody>
    </table>
